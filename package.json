{
  "name": "qris-static-with-amount",
  "version": "1.0.0",
  "description": "TypeScript library for inserting transaction amounts into static QRIS (QR Code Indonesian Standard) strings. Supports GoPay, OVO, Dana, ShopeePay, and all Indonesian e-wallets.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "QUICKSTART.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "qris",
    "qris-generator",
    "qris-payment",
    "qris-indonesia",
    "payment",
    "payment-gateway",
    "indonesia",
    "indonesian-payment",
    "qr-code",
    "qrcode",
    "emv",
    "emvco",
    "tlv",
    "gopay",
    "ovo",
    "dana",
    "shopeepay",
    "linkaja",
    "e-wallet",
    "digital-payment",
    "bank-indonesia",
    "typescript",
    "type-safe",
    "static-qris",
    "dynamic-qr",
    "merchant",
    "pos",
    "point-of-sale",
    "checkout",
    "transaction"
  ],
  "author": "rukandax",
  "license": "MIT",
  "homepage": "https://github.com/rukandax/qris-static-with-amount#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rukandax/qris-static-with-amount.git"
  },
  "bugs": {
    "url": "https://github.com/rukandax/qris-static-with-amount/issues"
  },
  "devDependencies": {
    "@jest/environment": "^30.2.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-environment-node": "^29.7.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
